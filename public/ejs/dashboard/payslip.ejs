<div class="col-md-10">
    <fieldset>
        <legend>Payslips</legend>
        <div class="form-group">
            <label class="col-md-2 control-label" for="text-field">Name:</label>
            <div class="col-md-10">
                <input class="form-control"  type="text">
            </div>
        </div>

        <div class="form-group">
            <label class="col-md-2 control-label" for="text-field">Reg No.</label>
            <div class="col-md-10">
                <input class="form-control"  type="text">

            </div>

        </div>
        <div class="form-group">
            <label class="col-md-2 control-label" for="text-field">Monthly Salary</label>
            <div class="col-md-10">
                <input class="form-control"  type="number">

            </div>

        </div>

        <div class="form-group">
            <label class="col-md-2 control-label">Bank Account Number</label>
            <div class="col-md-10">
                <input class="form-control" type="text">
                <br>

                <div ng-if="message.content">
                    <div class="alert alert-danger" ng-if="message.error"><b>{{message.content}}</b></div>
                    <div class="alert alert-success" ng-if="!message.error">
                        <b>{{message.content}}</b>
                    </div>
                </div>
                <div class="action">
                    <button class="btn btn-primary signup">Submit</button>
                </div>
            </div>
        </div>



        <div class="panel-body">
            <div id="timechart" style="width:100%;height:300px"></div>
        </div>
</div>
